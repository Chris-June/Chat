{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/apps/api/vercel.ts",
      "continue": false
    },
    {
      "src": "/assets/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }

}
